@model RequestAddServerViewModel
@{
    ViewBag.Title = "RequestAssignIPInfo";
    ViewBag.Role = "Shift Head";
}

<section class="content-header">
    <h1>
        Detail Request Add New Server
    </h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action(" Index2", "Schedule" )"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Detail Request Add New Server</li>
    </ol>
</section>
<section class="content">
    <div class="box">
        <div class="box-body table-responsive">
            @using (Html.BeginForm("ProcessRequestAddServer", "Request", FormMethod.Post))
            {
                <dl class="dl-horizontal">
                    <dt>
                        From customer
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Customer, new { htmlAttributes = new { @class = "form-control" } })
                    </dd>
                    <dt>
                        Full Name
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.CustomerName, new { htmlAttributes = new { @class = "form-control" } })
                    </dd>
                    <dt>
                        Requested Time
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.RequestedTime, new { htmlAttributes = new { @class = "form-control" } })
                    </dd>
                    <dt>
                        Status
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.StatusName, new { htmlAttributes = new { @class = "form-control" } })
                    </dd>
                    @if (Model.AssignedPerson != null)
                    {
                    <dt>
                        Assigned Person Name
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.AssignedPerson, new { htmlAttributes = new { @class = "form-control" } })
                    </dd>
                    }
                    <dt>
                        Identification
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Identification, new { htmlAttributes = new { @class = "form-control" } })
                    </dd>
                    <dt>
                        Appointment Time
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.AppointmentTime, new { htmlAttributes = new { @class = "form-control" } })
                    </dd>
                    <dt>
                        Description
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Description, new { htmlAttributes = new { @class = "form-control" } })
                    </dd>
                    <dt style="color: #f56954">
                        <h4>Server Content</h4>
                    </dt>
                    <dd></dd>
                </dl>
                <div class="form-group" style="width: 1000px">
                    <table id="" class="table table-bordered table-striped  col-xs-offset-1">
                        <thead>
                            <tr>
                                <th>No</th>
                                <th>Maker</th>
                                <th>Power</th>
                                <th>Model</th>
                                <th>Size</th>
                                <th>Bandwidth</th>
                                <th>Serial Number</th>
                                <th>Part Number</th>
                                <th>Memory</th>
                                <th>Default IP</th>
                                <th>Location</th>
                            </tr>
                        </thead>
                        <tbody>
                            @for (int i = 0; i < Model.Servers.Count; i++)
                            {
                            <tr>
                                <td>
                                    @Html.Raw(i + 1)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => Model.Servers[i].Maker, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.HiddenFor(modelItem => modelItem.Servers[i].Maker)
                                    @Html.HiddenFor(modelItem => modelItem.Servers[i].ServerCode)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => Model.Servers[i].Power, new { htmlAttributes = new { @class = "form-control" } })W
                                    @Html.HiddenFor(modelItem => modelItem.Servers[i].Power)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => Model.Servers[i].Model, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.HiddenFor(modelItem => modelItem.Servers[i].Model)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => Model.Servers[i].Size, new { htmlAttributes = new { @class = "form-control" } })U
                                    @Html.HiddenFor(modelItem => modelItem.Servers[i].Size)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => Model.Servers[i].Bandwidth, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.HiddenFor(modelItem => modelItem.Servers[i].Bandwidth)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => Model.Servers[i].SerialNumber, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.HiddenFor(modelItem => modelItem.Servers[i].SerialNumber)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => Model.Servers[i].PartNumber, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.HiddenFor(modelItem => modelItem.Servers[i].PartNumber)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => Model.Servers[i].Memory, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.HiddenFor(modelItem => modelItem.Servers[i].Memory)
                                </td>
                                <td>
                                    <input type="hidden" value="true" name="defaultIp" />
                                    <label><a href="#" data-toggle="modal" data-target="#assignip-modal">Assign Default IP</a></label>
                                </td>
                                <td>
                                    <input type="hidden" value="true" name="location" />
                                    <label><a href="#" data-toggle="modal" data-target="#assignlocation-modal">Assign Location</a></label>
                                </td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>


                @Html.HiddenFor(model => model.RequestCode)
                if (Model.StatusCode == Constants.StatusCode.REQUEST_WAITING || Model.StatusCode == Constants.StatusCode.REQUEST_PROCESSING)
                {
                    <button type="submit" class="btn btn-info"><i class="fa fa-external-link"></i> Export Procedure</button>
                    <button type="submit"id="btnApprove" class="btn btn-primary" name="Approve" value="Approve"">Approve</button>
                    <button type="submit" class="btn btn-danger">Reject</button>
                }
            }
        </div><!-- /.box-body -->
    </div><!-- /.box -->
</section>
